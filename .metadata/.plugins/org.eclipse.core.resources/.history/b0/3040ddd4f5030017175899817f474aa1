package models;

public abstract class Vehiculo  implements Movible, Arrancable{

	//Atributos
	private String marca;
	private String color;
	private String matricula;
	private double vAct;
	private double vMax;
	private String telefonoPropietrio;
	
	
	// public Vehiculo (String marca, String matricula, String color, double vAct, double vMax, String telefonoPropietario ){
	
	//this.marca = marca;
	//this.color = color;
	//this.matricula = matricula;
	//this.vAct = vAct;
	//this.vMax = vMax;
	//this.telefonoPropietario = telefonoPropietario;
	
	
	

	
	
	
//}

@Override

	public void Arrancar(){
	System.out.println("El vehículo con matrícula "+this.matricula+"  ha arrancado");
}

	public void Parar(){
		System.out.println("El vehículo con matrícula "+this.matricula+"  ha parado");
}

	public void Accelerar(){
		this.vAct = vAct+10;
}

	public void Frenar(){
		this.vAct = vAct-10;
}

}
	

	

